{
  "name": "chat-with-people-backend",
  "version": "1.0.0",
  "description": "Backend codes for the project ChatWithPeople",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-jwt": "^5.3.1",
    "graphql": "^0.13.2",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.2.2",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.5",
    "password-hash": "^1.2.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-abstract": "^1.1.1",
    "redux-saga": "^0.16.0",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "smart-input-validator": "^1.1.11",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "timeago.js": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-watch": "^3.1.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^22.4.4",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "webpack": "^3.12.0"
  },
  "scripts": {
    "db:uninstall": "babel-node node_modules/.bin/sequelize db:drop",
    "db:install": "babel-node node_modules/.bin/sequelize db:create && babel-node node_modules/.bin/sequelize db:migrate",
    "db:reinstall": "npm run db:uninstall && npm run db:install",
    "db:migration:create": "babel-node node_modules/.bin/sequelize migration:generate",
    "reinstallNodeModules": "rm -rf node_modules && npm install",
    "test": "jest",
    "config:nodemailer": "cp configs/nodemailer.example.js src/config/nodemailer.js",
    "config:sequelize": "babel-node node_modules/.bin/sequelize init",
    "config:init": "mkdir src/config && npm run config:nodemailer && npm run config:sequelize",
    "create:migration": "babel-node node_modules/.bin/sequelize migration:generate",
    "eslint": "node node_modules/eslint/bin/eslint.js --ext .js --parser babel-eslint src/** resources/js/** tests/** configs/** database/**",
    "eslint:configs": "node node_modules/eslint/bin/eslint.js webpack.config.js postcss.config.js workbox-config.js",
    "eslint:all": "npm run eslint && npm run eslint:configs",
    "build": "rm -rf build && npm run webpack:production && babel src --out-dir build --minified && cp -R src/config/sequelize.json build/config/sequelize.json && cp -R src/public/ build/public",
    "serve": "node build/server.js",
    "start:production": "npm run build && npm run serve",
    "start": "nodemon --exec babel-node ./src/server.js",
    "webpack": "babel-node node_modules/webpack/bin/webpack.js",
    "webpack:watch": "babel-node node_modules/webpack/bin/webpack.js --watch --config webpack-dev.config.js",
    "webpack:production": "babel-node node_modules/webpack/bin/webpack.js --config webpack-prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aprilmintacpineda/chat-with-people-backend.git"
  },
  "keywords": [
    "chat",
    "nodejs",
    "express",
    "sequelize",
    "socket.io",
    "realtime"
  ],
  "author": "April Mintac Pineda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aprilmintacpineda/chat-with-people-backend/issues"
  },
  "homepage": "https://github.com/aprilmintacpineda/chat-with-people-backend#readme"
}
